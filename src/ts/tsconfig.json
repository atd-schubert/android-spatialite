    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "../../www",
    "noImplicitAny": false,
    "removeComments": true,
    "preserveConstEnums": true,
    "declaration": true,
    "sourceMap": true
