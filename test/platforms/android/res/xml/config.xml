<?xml version='1.0' encoding='utf-8'?>
<widget id="org.yagajs.spatialite.test" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="SpatiaLitePlugin">
        <param name="android-package" value="org.yagajs.spatialite.SpatiaLitePlugin" />
    </feature>
    <feature name="SpatialitePlugin">
        <param name="android-package" value="org.yagajs.spatialite.SpatialitePlugin" />
    </feature>
    <allow-intent href="market:*" />
    <name>Yaga Spatialite Software tests</name>
    <description>
        Softwaretest app for Yaga spatialite
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Yagajs.org
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
